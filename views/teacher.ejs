<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title>Form Teacher</title>
  </head>
  <body>
  <h1>Form Teacher</h1>
  <table border="1">
    <tr>
      <th> Full Name </th>
      <th> Address </th>
      <th> City </th>
    </tr>
    <% data.forEach(function(teacher){ %>
      <tr>
        <td><%= teacher.fullname %></td>
        <td><%= teacher.address %></td>
        <td><%= teacher.city %></td>
      </tr>
    <% }) %>
  </table>
  </body>

</html>
